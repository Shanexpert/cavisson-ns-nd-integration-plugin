# ------------------ || Main Targets || -----------------------
clean:
	rm -rf $(BDIR)

build: lib init main

# Adding new target lib because on switching form one branch to another branch
#  facing issue - library changes is not same in all and hence build failed
lib:
	cd $(__core_base_dir)/topology/topolib_v1; make 
	cd $(__core_base_dir)/topology/topolib; make
	cd $(__core_base_dir)/libnscore; make

init:
	# Create build and its sub-dirs for compilation and package bundling
	mkdir -p $(BIDIR)
	mkdir -p $(BODIR)
	mkdir -p $(BBDIR)
	mkdir -p $(BTDIR)
	mkdir -p $(BSDIR)
	mkdir -p $(LOCAL_BIN_DIR)
	mkdir -p $(BMKDIR)
	
	mkdir -p $(BDIR)/etc 
	mkdir -p $(BSDIR)/tr069/conf 
	mkdir -p $(BSDIR)/tr069/templates
	mkdir -p $(BSDIR)/tr069/samples 
	mkdir -p $(BSDIR)/tr069/data 
	mkdir -p $(BSDIR)/tr069/schema

main:   $(NS_BINS)
	# Copy into dir 'build/etc'
	cp $(EXPORT_ETCFILES) $(BDIR)/etc
	
	# Copy into dir 'build/bin'
	cp $(EXPORT_BINFILES) $(BBDIR)
	chmod 755 $(BBDIR)/nsi_ns_upgrade  
	chmod 755 $(BBDIR)/nsi_show_config 
	chmod 755 $(BBDIR)/nsi_har_to_webcoach
	
	# Copy into dir 'build/include'
	cp $(EXPORT_INCFILES) $(BIDIR) 
	
	# Copy into dir 'build/sample'
	cp $(EXPORT_SAMPLEFILES) $(BSDIR) 
	
	# Copy into dir 'build/tools'
	cp $(EXPORT_TOOLFILES) $(BTDIR)

increas_build:
	$(shell $(__tool_dir)/build_version -i)

get_build_id:
	$(eval BUILD_RELEASE=$(shell $(__tool_dir)/build_version -r))
	$(eval BUILD_VERSION=$(shell $(__tool_dir)/build_version -b))
	$(eval BUILD_ID=$(BUILD_RELEASE).$(BUILD_VERSION)_$(NS_RELEASE))

release: get_build_id
	- rm -f $(BMKDIR)/*	
	- rm -f $(BDIR)/capture.*.gz
	- rm -f $(BDIR)/dbfunc.*.gz
	- rm -f $(BDIR)/ns_pg_bulkload.*.gz
	- rm -rf $(BDIR)/webapps/*
	- rm -rf $(BDIR)/rbu
	
	mkdir -p $(BMKDIR)
	mkdir -p $(BDIR)/etc $(BDIR)/docs $(BDIR)/samples \
		$(BDIR)/sys $(BDIR)/events $(BDIR)/include $(BDIR)/templates \
		$(BDIR)/templates/excel_templates $(BDIR)/templates/jenkins $(BDIR)/tools $(BDIR)/templates/thresholdTemplates \
		$(BDIR)/thirdparty $(BDIR)/cert $(BDIR)/server_management $(BDIR)/cert/custom \
		$(BDIR)/samples/tr069/conf $(BDIR)/samples/tr069/templates \
		$(BDIR)/samples/tr069/samples $(BDIR)/samples/tr069/data $(BDIR)/samples/tr069/schema \
		$(BDIR)/templatesJSON mkdir $(BDIR)/etc/dbconf $(BDIR)/samples/replay/profiles/default \
