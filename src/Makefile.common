release_bin: get_build_id
	- rm -f $(BBDIR)/netstorm.*$(NS_RELEASE).bin
	cd $(BDIR); echo "NS_RELEASE=$(NS_RELEASE)" > $(BMKDIR)/upgrade.spec
	cd $(BDIR); echo "netstorm=netstorm.$(BUILD_RELEASE).$(BUILD_VERSION).tar.gz" >> $(BMKDIR)/upgrade.spec
	cd $(BDIR); echo "build=netstorm${SSL_VERSION}.$(BUILD_ID).bin" >> $(BMKDIR)/upgrade.spec
		
	if [[ $(NS_RELEASE) == "Redhat_64" ]]; then \
		cp $(LDIR)/librhelnsc++.so $(BMKDIR)/libnsc++.so; \
	else \
		cp $(LDIR)/libnsc++.so $(BMKDIR)/libnsc++.so; \
	fi
	cp $(LDIR)/libdiffc++.so $(BMKDIR)/libdiffc++.so
	
	cp etc/version $(TDIR)/nsi_get_linux_release_ex $(TDIR)/nsi_upgrade.sh nsi_migrate.sh \
		nsi_migrate_scen nsi_migrate_cshm_conf $(TOOL_SRC)/nsi_change_custom_mon_keyword $(BBDIR)/nsu_two_step_encrypt $(BMKDIR)
		
	$(__cav_make_cmd) --current $(BMKDIR) $(BDIR)/netstorm${SSL_VERSION}.$(BUILD_ID).bin \
		"Netstorm ${BUILD_RELEASE} Build ${BUILD_VERSION}" ./nsi_upgrade.sh
		
	make -f Makefile.common do_export

do_export: get_build_id
	mkdir -p $(__export_dir)
	mkdir -p $(__export_dir)/mk
	echo "NS_RELEASE = $(NS_RELEASE), BUILD_ID = $(BUILD_ID)"	
	- rm -f $(__export_dir)/netstorm.*$(NS_RELEASE).bin
	- rm -f $(__export_dir)/mk/netstorm.*.tar.gz
	
	cp $(BDIR)/netstorm.$(BUILD_ID).bin $(__export_dir)
	cp $(BMKDIR)/netstorm.*.tar.gz $(__export_dir)/mk/
	cp $(BBDIR)/nsu_two_step_encrypt  $(__export_dir)/mk/
