#! /bin/bash
# This shell is used for generation of RBU Metrics Report.
# Author --- Pooja Singh

if [ $# -ne 1 ];then
  echo "Usage: rbuReportGenrate.sh <Current_TestSuit_ID>"
  exit 1
fi

TESTSUIT_ID=$1

java -cp $NS_WDIR/webapps/netstorm/WEB-INF/lib/netstorm_bean.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/gson-2.2.2.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/jackson-all-1.9.0.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/commons-codec-1.6.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/commons-dbcp-1.4.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/commons-pool-1.6.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/jcl-over-slf4j-1.7.12.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/json_simple-1.1.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/jxl-2.6.10.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/logback-classic-1.1.3.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/logback-core-1.1.3.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/poi-ooxml-3.14.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/poi-ooxml-schemas-3.14.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/postgresql-9.1-901.jdbc4.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/slf4j-api-1.7.12.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/commons-jexl-2.1.1.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/jxls-2.4.1.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/jxls-poi-1.0.13.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/harlib-jackson-1.0b10.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/poi-3.14.jar:$NS_WDIR/webapps/netstorm/WEB-INF/lib/javax.mail-1.5.4.jar -DNS_WDIR=$NS_WDIR pac1.Bean.RBUPageMetricsReport $TESTSUIT_ID

exit $?
